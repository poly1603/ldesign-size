{
  "name": "@ldesign/size-react",
  "version": "0.1.0",
  "description": "Size management for React - Hooks, context, and components",
  "keywords": [
    "react",
    "hooks",
    "context",
    "size",
    "ldesign",
    "typescript"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/size/packages/react#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/size/packages/react"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./hooks/*": {
      "types": "./es/hooks/*.d.ts",
      "import": "./es/hooks/*.js",
      "require": "./lib/hooks/*.cjs"
    },
    "./context": {
      "types": "./es/context/index.d.ts",
      "import": "./es/context/index.js",
      "require": "./lib/context/index.cjs"
    },
    "./components/*": {
      "types": "./es/components/*.d.ts",
      "import": "./es/components/*.js",
      "require": "./lib/components/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "dev": "ldesign-builder build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf es lib dist coverage"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "@ldesign/size-core": "workspace:*"
  },
  "dependencies": {
    "lucide-react": "^0.460.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/size-core": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitest": "^2.0.0"
  }
}

